{
  "name": "ArmoryWow",
  "version": "1.0.0",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd Back && npm install",
    "install:frontend": "cd Front && npm install",
    "start:backend": "cd Back && npm run dev",
    "start:frontend": "cd Front && npm run dev",
    "dev": "concurrently  \"npm run start:frontend\" \"npm run start:backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "expo": "^53.0.16"
  }
}
